{
  "rules": {
    "links": {
      "$uid": {
        ".read": true,
        ".write": "auth.uid === $uid"  
      }
    },
    "stream_meta": {
      ".read": true,
      ".write": "auth.uid !== null"
    },
    "streams": {
      "$uid": {
        ".read": true,
        ".write": "auth.uid === $uid"
      }
    }
  }
}